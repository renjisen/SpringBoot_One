<mapper namespace="com.rjs.mapper.partMapper.CheckDataMapper">

    <insert id="insertFirstCheckData" >
        insert into firstcheckdata (recordmethod,firstcheckdata,checkid) values (#{recordmethod2},#{checkdata},#{checkid2});
    </insert>

    <insert id="insertZhongCheckData" >
        insert into zhongcheckdata (recordmethod,zhongcheckdata,checkid) values (#{recordmethod2},#{checkdata},#{checkid2});
    </insert>

    <insert id="insertEndCheckData" >
        insert into endcheckdata (recordmethod,endcheckdata,checkid) values (#{recordmethod2},#{checkdata},#{checkid2});
    </insert>

</mapper>